  <display-name>WebAppIR</display-name>
  <welcome-file-list>
    <welcome-file>Index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>searchIR</servlet-name>
    <servlet-class>com.ir.nitesh.search.SearchIR</servlet-class>
  </servlet>
   
  <servlet-mapping>
    <servlet-name>searchIR</servlet-name>
